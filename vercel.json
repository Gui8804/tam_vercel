{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/list",
      "dest": "api/main.py",
      "methods": ["GET"]
    },
    {
      "src": "/events/add",
      "dest": "api/main.py",
      "methods": ["POST"]
    },
    {
      "src": "/events/edit",
      "dest": "api/main.py",
      "methods": ["PUT"]
    }
  ]
}
